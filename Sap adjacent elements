WAP TO SWAP THE ADJACENT ELEMENTS IN AN ARRAY

#include<stdio.h>
void main()
{
    int a[10],ele,i,t;
    ele=sizeof(a)/sizeof(a[0]);
    printf("enter the elements\n");
    for(i=0;i<ele;i++)
        scanf("%d",&a[i]);
    for(i=0;i<=ele-1;i=i+2)
    {
          t=a[i];
          a[i]=a[i+1];
          a[i]=t;
    }
    printf("after swaping:\n");
    for(i=0;i,ele;i++)
        printf("%d",a[i]);
}
